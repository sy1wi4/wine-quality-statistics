@

\item funkcja wyliczajaca mode
<<>>=
Mode <- function(v) {
  uniqv <- unique(v)
  uniqv[which.max(tabulate(match(v, uniqv)))]
}
@

\item funkcja obliczajaca, ile jest wartosci odstajacych
<<>>=
outliers <- function(x){
  length(which(x >  mean(x) + 3 * sd(x) | x < mean(x) - 3 * sd(x))  ) / length(x)
}
@




\end{enumerate}

